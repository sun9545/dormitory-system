{
    "name": "sun9545/dormitory-management-system",
    "description": "Smart Dormitory Management System - Web Application",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4.0",
        "phpoffice/phpspreadsheet": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Models\\": "models/",
            "App\\Utils\\": "utils/",
            "App\\Controllers\\": "controllers/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
